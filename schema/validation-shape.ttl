@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix kastle: <http://kastle.com/> .

kastle:CountyShape
  a sh:NodeShape ;
  sh:targetClass schema:Place ;
  sh:property [    sh:path kastle:hasCountyName ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] ;
  sh:property [    sh:path kastle:hasCountyCode ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] ;
  sh:property [    sh:path kastle:hasCountyFIPS ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] ;
  sh:property [    sh:path kastle:hasCountyID ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] ;
  sh:property [    sh:path kastle:hasStateCode ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] ;
  sh:property [    sh:path kastle:hasStateName ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] .

kastle:StateShape
  a sh:NodeShape ;
  sh:targetClass schema:Place ;
  sh:property [    sh:path kastle:hasStateCode ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] ;
  sh:property [    sh:path kastle:hasStateFIPS ;    sh:datatype xsd:string ;    sh:minCount 1 ;  ] .
